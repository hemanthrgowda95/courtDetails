  <Sequence DisplayName="example" sap2010:WorkflowViewState.IdRef="Sequence_4">
    <DoWhile DisplayName="Check valid init day" sap2010:WorkflowViewState.IdRef="DoWhile_1">
      <DoWhile.Variables>
        <Variable x:TypeArguments="x:Boolean" Name="validInitDay" />
      </DoWhile.Variables>
      <DoWhile.Condition>[Not validInitDay]</DoWhile.Condition>
      <Sequence sap2010:WorkflowViewState.IdRef="Sequence_3">
        <Sequence.Variables>
          <Variable x:TypeArguments="s:DateTime" Name="dateValue" />
          <Variable x:TypeArguments="ui:GenericValue" Name="typedDay" />
        </Sequence.Variables>
        <ui:InputDialog Options="{x:Null}" DisplayName="Input dialog" sap2010:WorkflowViewState.IdRef="InputDialog_1" IsPassword="False" Label="Please insert a date with the following format (dd/mm/aaaa): " Title="Hi">
          <ui:InputDialog.Result>
            <OutArgument x:TypeArguments="ui:GenericValue">[typedDay]</OutArgument>
          </ui:InputDialog.Result>
        </ui:InputDialog>
        <If Condition="[DateTime.TryParseExact(typedDay, &quot;dd/MM/yyyy&quot;, Nothing, Nothing, dateValue)]" sap2010:WorkflowViewState.IdRef="If_1">
          <If.Then>
            <Sequence sap2010:WorkflowViewState.IdRef="Sequence_1">
              <Sequence.Variables>
                <Variable x:TypeArguments="x:String" Name="dateTimeInit" />
              </Sequence.Variables>
              <Assign sap2010:WorkflowViewState.IdRef="Assign_1">
                <Assign.To>
                  <OutArgument x:TypeArguments="x:String">[dateTimeInit]</OutArgument>
                </Assign.To>
                <Assign.Value>
                  <InArgument x:TypeArguments="x:String">[DateTime.ParseExact(typedDay, "dd/MM/yyyy", Nothing).ToString("dd.MM.yyyy")]</InArgument>
                </Assign.Value>
              </Assign>
              <Assign sap2010:WorkflowViewState.IdRef="Assign_2">
                <Assign.To>
                  <OutArgument x:TypeArguments="x:Boolean">[validInitDay]</OutArgument>
                </Assign.To>
                <Assign.Value>
                  <InArgument x:TypeArguments="x:Boolean">True</InArgument>
                </Assign.Value>
              </Assign>
            </Sequence>
          </If.Then>
          <If.Else>
            <Sequence sap2010:WorkflowViewState.IdRef="Sequence_2">
              <Sequence.Variables>
                <Variable x:TypeArguments="x:Boolean" Name="validEndDay" />
              </Sequence.Variables>
              <ui:MessageBox ChosenButton="{x:Null}" Buttons="Ok" Caption="Atenção!" DisplayName="Message box" sap2010:WorkflowViewState.IdRef="MessageBox_1" Text="Invalid date." TopMost="True" />
            </Sequence>
          </If.Else>
        </If>
      </Sequence>
    </DoWhile>
